from math import cos, sin, tan
n = float(input('Digite um número: '))
print(f'O valor do seno é {sin(n)}')
print(f'O valor do cosseno é {cos(n)}')
print(f'O valor da tangente é {tan(n)}')

import math
n = float(input('Digite um número: '))
print('O valor do seno é: {:.2f}' .format(math.sin(n)))
print('O valor do cosseno é: {:.2f}' .format(math.cos(n)))
print('O valor dA tangente é: {:.2f}' .format(math.tan(n)))
